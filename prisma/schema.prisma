generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String        @id @default(uuid())
  name        String
  image       String
  testimonial Testimonial[]
}

model Testimonial {
  id          String   @id @default(uuid())
  userId      String
  description String
  createdAt   DateTime @default(now())
  user        User     @relation(fields: [userId], references: [id])
}
